{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus datasource for amazing-gitlab-exporter metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "1.0.0" },
    { "type": "panel", "id": "table", "name": "Table", "version": "1.0.0" }
  ],
  "id": null,
  "uid": "age-dora-metrics",
  "title": "DORA Metrics",
  "description": "DevOps Research and Assessment (DORA) metrics from amazing-gitlab-exporter (Ultimate tier)",
  "tags": ["gitlab", "dora", "devops", "amazing-gitlab-exporter"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Deployment Frequency",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg(age_dora_deployment_frequency{project=~\"$project\", environment_tier=~\"$environment_tier\"})",
          "legendFormat": "Deploys/Day",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.14 },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 7 }
            ]
          },
          "color": { "mode": "thresholds" },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Lead Time for Changes",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg(age_dora_lead_time_for_changes_seconds{project=~\"$project\", environment_tier=~\"$environment_tier\"})",
          "legendFormat": "Lead Time",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 86400 },
              { "color": "orange", "value": 604800 },
              { "color": "red", "value": 2592000 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "Mean Time to Restore (MTTR)",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg(age_dora_time_to_restore_service_seconds{project=~\"$project\", environment_tier=~\"$environment_tier\"})",
          "legendFormat": "MTTR",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 3600 },
              { "color": "orange", "value": 86400 },
              { "color": "red", "value": 604800 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "Change Failure Rate",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg(age_dora_change_failure_rate{project=~\"$project\", environment_tier=~\"$environment_tier\"}) * 100",
          "legendFormat": "Failure Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "orange", "value": 15 },
              { "color": "red", "value": 30 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 5,
      "title": "Deployment Frequency Trend",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg by (project) (age_dora_deployment_frequency{project=~\"$project\", environment_tier=~\"$environment_tier\"})",
          "legendFormat": "{{project}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "scheme",
            "showPoints": "auto",
            "pointSize": 5,
            "axisLabel": "Deploys / Day"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "lastNotNull"] }
      }
    },
    {
      "id": 6,
      "title": "Lead Time for Changes Trend",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg by (project) (age_dora_lead_time_for_changes_seconds{project=~\"$project\", environment_tier=~\"$environment_tier\"})",
          "legendFormat": "{{project}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "scheme",
            "showPoints": "auto",
            "pointSize": 5,
            "axisLabel": "Lead Time"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "lastNotNull"] }
      }
    },
    {
      "id": 7,
      "title": "MTTR & Change Failure Rate Trend",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg(age_dora_time_to_restore_service_seconds{project=~\"$project\", environment_tier=~\"$environment_tier\"})",
          "legendFormat": "MTTR",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "scheme",
            "showPoints": "auto",
            "pointSize": 5,
            "axisLabel": "MTTR"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 8,
      "title": "DORA Monthly Comparison",
      "type": "table",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg by (project) (avg_over_time(age_dora_deployment_frequency{project=~\"$project\", environment_tier=~\"$environment_tier\"}[30d]))",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        },
        {
          "expr": "avg by (project) (avg_over_time(age_dora_lead_time_for_changes_seconds{project=~\"$project\", environment_tier=~\"$environment_tier\"}[30d]))",
          "legendFormat": "",
          "refId": "B",
          "format": "table",
          "instant": true
        },
        {
          "expr": "avg by (project) (avg_over_time(age_dora_time_to_restore_service_seconds{project=~\"$project\", environment_tier=~\"$environment_tier\"}[30d]))",
          "legendFormat": "",
          "refId": "C",
          "format": "table",
          "instant": true
        },
        {
          "expr": "avg by (project) (avg_over_time(age_dora_change_failure_rate{project=~\"$project\", environment_tier=~\"$environment_tier\"}[30d])) * 100",
          "legendFormat": "",
          "refId": "D",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "Value #A" }, "properties": [{ "id": "displayName", "value": "Deploy Freq (30d avg)" }, { "id": "unit", "value": "short" }, { "id": "decimals", "value": 2 }] },
          { "matcher": { "id": "byName", "options": "Value #B" }, "properties": [{ "id": "displayName", "value": "Lead Time (30d avg)" }, { "id": "unit", "value": "s" }] },
          { "matcher": { "id": "byName", "options": "Value #C" }, "properties": [{ "id": "displayName", "value": "MTTR (30d avg)" }, { "id": "unit", "value": "s" }] },
          { "matcher": { "id": "byName", "options": "Value #D" }, "properties": [{ "id": "displayName", "value": "CFR % (30d avg)" }, { "id": "unit", "value": "percent" }, { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 15 }] } }, { "id": "custom.cellOptions", "value": { "type": "color-background" } }] }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "project", "desc": false }],
        "footer": { "show": false }
      },
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "organize", "options": { "excludeByName": { "Time": true }, "renameByName": { "project": "Project" } } }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false
      },
      {
        "name": "project",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_dora_deployment_frequency, project)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "environment_tier",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_dora_deployment_frequency{project=~\"$project\"}, environment_tier)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      }
    ]
  },
  "time": { "from": "now-90d", "to": "now" },
  "timepicker": {},
  "refresh": "1h",
  "schemaVersion": 39,
  "version": 1
}
