{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus datasource for amazing-gitlab-exporter metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "1.0.0" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "1.0.0" },
    { "type": "panel", "id": "table", "name": "Table", "version": "1.0.0" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "1.0.0" }
  ],
  "id": null,
  "uid": "age-job-perf",
  "title": "Job Performance",
  "description": "GitLab CI/CD job performance metrics from amazing-gitlab-exporter",
  "tags": ["gitlab", "jobs", "ci-cd", "amazing-gitlab-exporter"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Top 20 Slowest Jobs (P95 Duration)",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "topk(20, histogram_quantile(0.95, sum by (job_name, le) (rate(age_job_duration_seconds_bucket{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\"}[$__rate_interval]))))",
          "legendFormat": "{{job_name}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "continuous-BlYlRd" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 120 },
              { "color": "orange", "value": 300 },
              { "color": "red", "value": 600 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "minVizWidth": 8,
        "minVizHeight": 16,
        "valueMode": "color"
      }
    },
    {
      "id": 2,
      "title": "Job Failure Rate",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sort_desc(\n  sum by (job_name, stage) (\n    increase(age_job_run_count{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\", job_name=~\"$job_name\"}[$__range])\n  )\n)",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        },
        {
          "expr": "sort_desc(\n  sum by (job_name, stage) (\n    age_job_status{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\", job_name=~\"$job_name\", status=\"failed\"}\n  )\n  / sum by (job_name, stage) (\n    age_job_status{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\", job_name=~\"$job_name\"}\n  ) * 100\n)",
          "legendFormat": "",
          "refId": "B",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "Value #A" }, "properties": [{ "id": "displayName", "value": "Total Runs" }, { "id": "unit", "value": "short" }] },
          { "matcher": { "id": "byName", "options": "Value #B" }, "properties": [{ "id": "displayName", "value": "Failure %" }, { "id": "unit", "value": "percent" }, { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 20 }] } }, { "id": "custom.cellOptions", "value": { "type": "color-background" } }] }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Failure %", "desc": true }],
        "footer": { "show": false }
      },
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "organize", "options": { "excludeByName": { "Time": true }, "renameByName": { "job_name": "Job", "stage": "Stage" } } }
      ]
    },
    {
      "id": 3,
      "title": "Stage Duration (Stacked)",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (stage) (histogram_quantile(0.50, sum by (stage, le) (rate(age_job_duration_seconds_bucket{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\"}[$__rate_interval]))))",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 80,
            "gradientMode": "none",
            "showPoints": "never",
            "stacking": { "mode": "normal", "group": "A" },
            "axisLabel": "Duration"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean"] }
      }
    },
    {
      "id": 4,
      "title": "Runner Utilization",
      "type": "piechart",
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (runner_type) (increase(age_job_run_count{project=~\"$project\", ref=~\"$ref\"}[$__range]))",
          "legendFormat": "{{runner_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "pieType": "donut",
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      }
    },
    {
      "id": 5,
      "title": "Queue Time by Stage",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 6, "x": 18, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (stage, le) (rate(age_job_queued_duration_seconds_bucket{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\"}[$__rate_interval])))",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "axisLabel": "Queue Time"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 6,
      "title": "Artifact Sizes by Job",
      "type": "bargauge",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "topk(15, sum by (job_name) (age_job_artifact_size_bytes{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\"}))",
          "legendFormat": "{{job_name}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "continuous-blues" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 104857600 },
              { "color": "red", "value": 524288000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "valueMode": "color"
      }
    },
    {
      "id": 7,
      "title": "Failure Reasons",
      "type": "table",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sort_desc(sum by (job_name, failure_reason) (age_job_status{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\", status=\"failed\", failure_reason!=\"\"}))",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "Value" }, "properties": [{ "id": "displayName", "value": "Count" }] }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Count", "desc": true }],
        "footer": { "show": false }
      },
      "transformations": [
        { "id": "organize", "options": { "excludeByName": { "Time": true }, "renameByName": { "job_name": "Job", "failure_reason": "Failure Reason" } } }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false
      },
      {
        "name": "project",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_job_status, project)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "ref",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_job_status{project=~\"$project\"}, ref)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "stage",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_job_status{project=~\"$project\", ref=~\"$ref\"}, stage)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "job_name",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_job_status{project=~\"$project\", ref=~\"$ref\", stage=~\"$stage\"}, job_name)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "refresh": "30s",
  "schemaVersion": 39,
  "version": 1
}
