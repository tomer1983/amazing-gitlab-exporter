{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus datasource for amazing-gitlab-exporter metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "1.0.0" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "1.0.0" },
    { "type": "panel", "id": "table", "name": "Table", "version": "1.0.0" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "1.0.0" }
  ],
  "id": null,
  "uid": "age-repo-analytics",
  "title": "Repository Analytics",
  "description": "GitLab repository analytics from amazing-gitlab-exporter",
  "tags": ["gitlab", "repository", "analytics", "amazing-gitlab-exporter"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Language Breakdown",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sort_desc(avg by (language) (age_repository_language_percentage{project=~\"$project\"}))",
          "legendFormat": "{{language}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "pieType": "donut",
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      }
    },
    {
      "id": 2,
      "title": "Test Coverage Trend",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg by (project) (age_repository_coverage{project=~\"$project\"})",
          "legendFormat": "{{project}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "scheme",
            "showPoints": "auto",
            "pointSize": 5,
            "axisLabel": "Coverage %",
            "thresholdsStyle": { "mode": "line" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "green", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "min", "max"] }
      }
    },
    {
      "id": 3,
      "title": "Repository Size",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(age_repository_size_bytes{project=~\"$project\"})",
          "legendFormat": "Total Size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1073741824 },
              { "color": "red", "value": 5368709120 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "Current Coverage",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "avg(age_repository_coverage{project=~\"$project\"})",
          "legendFormat": "Avg Coverage",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "green", "value": 80 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 5,
      "title": "Commit Frequency",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (project) (increase(age_repository_commit_count{project=~\"$project\"}[1d]))",
          "legendFormat": "{{project}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 70,
            "gradientMode": "none",
            "showPoints": "never",
            "axisLabel": "Commits / Day"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["sum", "mean"] }
      }
    },
    {
      "id": 6,
      "title": "Repository Size Trend",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "age_repository_size_bytes{project=~\"$project\"}",
          "legendFormat": "{{project}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "scheme",
            "showPoints": "never",
            "axisLabel": "Size"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 7,
      "title": "Top Contributors",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "topk(20, sum by (author) (increase(age_contributor_commits_count{project=~\"$project\"}[$__range])))",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        },
        {
          "expr": "topk(20, sum by (author) (increase(age_contributor_additions{project=~\"$project\"}[$__range])))",
          "legendFormat": "",
          "refId": "B",
          "format": "table",
          "instant": true
        },
        {
          "expr": "topk(20, sum by (author) (increase(age_contributor_deletions{project=~\"$project\"}[$__range])))",
          "legendFormat": "",
          "refId": "C",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "Value #A" }, "properties": [{ "id": "displayName", "value": "Commits" }, { "id": "unit", "value": "short" }, { "id": "decimals", "value": 0 }] },
          { "matcher": { "id": "byName", "options": "Value #B" }, "properties": [{ "id": "displayName", "value": "Additions" }, { "id": "unit", "value": "short" }, { "id": "decimals", "value": 0 }, { "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Value #C" }, "properties": [{ "id": "displayName", "value": "Deletions" }, { "id": "unit", "value": "short" }, { "id": "decimals", "value": 0 }, { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Commits", "desc": true }],
        "footer": { "show": false }
      },
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "organize", "options": { "excludeByName": { "Time": true }, "renameByName": { "author": "Author" } } }
      ]
    },
    {
      "id": 8,
      "title": "Additions / Deletions by Contributor",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "topk(10, sum by (author) (increase(age_contributor_additions{project=~\"$project\"}[$__range])))",
          "legendFormat": "{{author}} (+)",
          "refId": "A",
          "instant": true
        },
        {
          "expr": "topk(10, sum by (author) (increase(age_contributor_deletions{project=~\"$project\"}[$__range])))",
          "legendFormat": "{{author}} (-)",
          "refId": "B",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "valueMode": "color"
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false
      },
      {
        "name": "project",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_repository_size_bytes, project)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      }
    ]
  },
  "time": { "from": "now-30d", "to": "now" },
  "timepicker": {},
  "refresh": "1h",
  "schemaVersion": 39,
  "version": 1
}
