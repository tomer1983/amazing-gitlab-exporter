{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus datasource for amazing-gitlab-exporter metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "1.0.0" },
    { "type": "panel", "id": "table", "name": "Table", "version": "1.0.0" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "1.0.0" }
  ],
  "id": null,
  "uid": "age-mr-analytics",
  "title": "Merge Request Analytics",
  "description": "GitLab merge request analytics from amazing-gitlab-exporter",
  "tags": ["gitlab", "merge-requests", "analytics", "amazing-gitlab-exporter"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Median Time to Merge",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(age_mr_time_to_merge_seconds_bucket{project=~\"$project\", target_branch=~\"$target_branch\"}[$__rate_interval])))",
          "legendFormat": "Median",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 86400 },
              { "color": "orange", "value": 259200 },
              { "color": "red", "value": 604800 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "MR Throughput (Merged / Day)",
      "type": "timeseries",
      "gridPos": { "h": 5, "w": 10, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(age_mr_throughput_count{project=~\"$project\", target_branch=~\"$target_branch\"}[1d]))",
          "legendFormat": "Merged MRs",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "fixed", "fixedColor": "green" },
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 70,
            "gradientMode": "none",
            "showPoints": "never",
            "axisLabel": "MRs Merged"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 3,
      "title": "Open MR Count",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(age_mr_status{project=~\"$project\", target_branch=~\"$target_branch\", state=\"opened\"})",
          "legendFormat": "Open",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 20 },
              { "color": "red", "value": 50 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "Avg Review Cycles",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 20, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(age_mr_review_cycles_count_bucket{project=~\"$project\", target_branch=~\"$target_branch\"}[$__rate_interval])))",
          "legendFormat": "Median Cycles",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 2 },
              { "color": "red", "value": 4 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 5,
      "title": "Time to Merge Distribution (P50 / P95)",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(age_mr_time_to_merge_seconds_bucket{project=~\"$project\", target_branch=~\"$target_branch\"}[$__rate_interval])))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(age_mr_time_to_merge_seconds_bucket{project=~\"$project\", target_branch=~\"$target_branch\"}[$__rate_interval])))",
          "legendFormat": "P95",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "scheme",
            "showPoints": "auto",
            "pointSize": 5,
            "axisLabel": "Time to Merge"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 6,
      "title": "MR Size Distribution",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(age_mr_changes_count_bucket{project=~\"$project\", target_branch=~\"$target_branch\"}[$__rate_interval])))",
          "legendFormat": "P50 Changes",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(age_mr_changes_count_bucket{project=~\"$project\", target_branch=~\"$target_branch\"}[$__rate_interval])))",
          "legendFormat": "P95 Changes",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "axisLabel": "Lines Changed"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean"] }
      }
    },
    {
      "id": 7,
      "title": "Open MR Aging",
      "type": "bargauge",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "topk(20, histogram_quantile(0.50, sum by (project, le) (rate(age_mr_open_duration_seconds_bucket{project=~\"$project\", target_branch=~\"$target_branch\"}[$__rate_interval]))))",
          "legendFormat": "{{project}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "continuous-BlYlRd" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 172800 },
              { "color": "red", "value": 604800 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "valueMode": "color"
      }
    },
    {
      "id": 8,
      "title": "MR Velocity Trend",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(age_mr_throughput_count{project=~\"$project\", target_branch=~\"$target_branch\"}[7d])) / 7",
          "legendFormat": "Avg MRs Merged / Day (7d)",
          "refId": "A"
        },
        {
          "expr": "sum(increase(age_mr_throughput_count{project=~\"$project\", target_branch=~\"$target_branch\"}[30d])) / 30",
          "legendFormat": "Avg MRs Merged / Day (30d)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "scheme",
            "showPoints": "never",
            "axisLabel": "MRs / Day"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean"] }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false
      },
      {
        "name": "project",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_mr_status, project)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "target_branch",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(age_mr_status{project=~\"$project\"}, target_branch)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "allValue": ".*"
      }
    ]
  },
  "time": { "from": "now-30d", "to": "now" },
  "timepicker": {},
  "refresh": "5m",
  "schemaVersion": 39,
  "version": 1
}
